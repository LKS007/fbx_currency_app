<div class="row text-center">
  <p>Force History</p>    
  <div class="history_table">
    <% if @currency_history.nil? %>
      NOTHING
    <% else %>
    <table class="table table-striped table-bordered">
      <thead>
        <tr>
          <th class="col">ID</th><th class="col">Rate</th><th class="col">Date To</th><th></th>
        </tr>
      </thead>
      <tbody>
        <% @currency_history.each do |story| %>
          <tr class="table-success">
            <td><%= story.id %></td>
            <td><%= story.rate %></td>
            <td><%= story.date_to %></td>
            <td>Delete</td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <% end %>
  </div>
</div>
